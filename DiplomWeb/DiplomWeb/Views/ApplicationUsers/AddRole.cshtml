@model IEnumerable<DiplomWeb.Models.ApplicationRole>

@{
    ViewBag.Title = "AddRole";
}

<h2>AddRole</h2>


<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Add Role", "ChoosedRole", new { role=item.Name, userId= ViewBag.UserId }) |
        </td>
    </tr>
}

</table>
